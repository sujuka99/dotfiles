	name = Ivan Yordanov
	email = i.p.yordanov@proton.me
[rerere]
	enabled = true
[credential]
	helper = cache
[alias]
	st = status
    clean-branches = "!git fetch -p && for branch in $(git for-each-ref --format '%(refname) %(upstream:track)' refs/heads | awk '$2 == "[gone]" {sub("refs/heads/", "", $1); print $1}'); do git branch -D $branch; done"
