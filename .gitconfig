[user]
	name = Ivan Yordanov
	email = ivan.yordanov@crystalloids
[rerere]
	enabled = true
[credential]
	helper = cache
[alias]
	st = status
    clean-branches = "!git fetch -p && for branch in $(git for-each-ref --format '%(refname) %(upstream:track)' refs/heads | awk '$2 == "[gone]" {sub("refs/heads/", "", $1); print $1}'); do git branch -D $branch; done"
	alias = !git help -a | sed '1,/aliases$/d'
	br = branch
	co = checkout
	ci = commit
[column]
	ui = auto
; [branch]
; 	sort = -comitterdate
[push]
	autoSetupRemote = true
[init]
	defaultBranch = main
